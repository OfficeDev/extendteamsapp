{
    "$schema": "https://office.visualstudio.com/MetaOS/_git/TPS-AppServices?path=/src/Services/Core/Common/Contract/src/Ingestion/Schemas/MOS_Schema_m365DevPreview.json&version=GBmain",
    "manifestVersion": "devPreview",
    "version": "1.0.2",
    "id": "${{TEAMS_APP_ID}}",
    "packageName": "com.microsoft.teams.extension",
    "developer": {
        "name": "Microsoft 365 CDA Team",
        "websiteUrl": "https://www.example.com",
        "privacyUrl": "https://www.example.com/termofuse",
        "termsOfUseUrl": "https://www.example.com/privacy"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "Northwind",
        "full": "Northwind suppliers"
    },
    "description": {
        "short": "Northwind suppliers",
        "full": "Northwind suppliers"
    },
    "accentColor": "#FFFFFF",
    "bots": [],
    "staticTabs": [
        {
            "entityId": "index",
            "name": "Tab",
            "contentUrl": "${{TAB_ENDPOINT}}/index.html#/tab",
            "websiteUrl": "${{TAB_ENDPOINT}}/index.html#/tab",
            "scopes": [
                "personal"
            ]
        },
        {
            "entityId": "launchPage",
            "name": "Northwind",
            "contentUrl": "${{TAB_ENDPOINT}}/index.html#/launchPage",
            "websiteUrl": "${{TAB_ENDPOINT}}/index.html#/launchPage",
            "scopes": [
                "personal"
            ]
        },
        {
            "entityId": "dialogPage",
            "name": "Dialog",
            "contentUrl": "${{TAB_ENDPOINT}}/index.html#/dialogPage",
            "websiteUrl": "${{TAB_ENDPOINT}}/index.html#/dialogPage",
            "scopes": [
                "personal"
            ]
        }
    ],
    "actions": {
        "actionGroups": [
            {
                "id": "northWindActionGroup",
                "displayName": "Northwind Action Group",
                "associatedActions": [
                    "all"
                ]
            }
        ],
        "contentActions": [
            {
                "id": "northwindOpenDialog",
                "displayName": "Add to supplier",
                "intent": "addTo",
                "supportedContent": {
                    "file": {
                        "extensions": [
                            "xlsx",
                            "doc",
                            "docx"
                        ]
                    }
                },
                "type": "openDialog",
                "url": "${{TAB_ENDPOINT}}/index.html#/dialogPage",
                "width": "small",
                "height": "large"
            },
            {
                "id": "northwindOpenPage",
                "displayName": "Related suppliers",
                "description": "Related Supplier openPage",
                "intent": "custom",
                "supportedContent": {
                    "file": {
                        "extensions": [
                            "xlsx"
                        ]
                    },
                    "folder": null
                },
                "type": "openPage",
                "pageId": "launchPage",
                "subPageId": ""
            }
        ]
    },
    "composeExtensions": [
        {
            "botId": "${{BOT_ID}}",
            "commands": [
                {
                    "id": "searchQuery",
                    "context": [
                        "compose",
                        "commandBox"
                    ],
                    "description": "Find a Northwind supplier",
                    "title": "Supplier search",
                    "type": "query",
                    "parameters": [
                        {
                            "name": "supplierME",
                            "title": "Supplier search",
                            "description": "Your search query",
                            "inputType": "text"
                        }
                    ]
                }
            ],
            "messageHandlers": [
                {
                    "type": "link",
                    "value": {
                        "domains": [
                            "*.botframework.com",
                            "*.northwindtraders.com"
                        ]
                    }
                }
            ]
        }
    ],
    "configurableTabs": [
        {
            "configurationUrl": "${{TAB_ENDPOINT}}/index.html#/tabconfig",
            "canUpdateConfiguration": true,
            "scopes": [
                "team",
                "groupchat"
            ],
            "context": [
                "channelTab",
                "meetingChatTab",
                "meetingDetailsTab",
                "meetingSidePanel",
                "meetingStage"
            ]
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "${{TAB_DOMAIN}}"
    ],
    "webApplicationInfo": {
        "id": "${{AAD_APP_CLIENT_ID}}",
        "resource": "api://${{TAB_DOMAIN}}/${{AAD_APP_CLIENT_ID}}"
    }
}